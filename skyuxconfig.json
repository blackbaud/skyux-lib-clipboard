{
  "$schema": "./node_modules/@skyux/config/skyuxconfig-schema.json",
  "name": "skyux-lib-clipboard",
  "mode": "easy",
  "compileMode": "aot",
  "skyuxModules": [
    "SkyErrorModule"
  ],
  "plugins": [
    "@skyux-sdk/builder-plugin-skyux"
  ],
  "appSettings": {
    "myLibrary": {
      "name": "Skyux Lib Clipboard"
    }
  },
  "app": {
    "title": "Sky Clipboard",
    "styles": [
      "@skyux/theme/css/sky.css"
    ]
  },
  "testSettings": {
    "unit": {
      "browserSet": "paranoid"
    }
  }
}
