{
  "$schema": "./node_modules/@skyux/config/skyuxconfig-schema.json",
  "name": "skyux-lib-clipboard",
  "mode": "easy",
  "compileMode": "aot",
  "plugins": [
    "@skyux-sdk/builder-plugin-skyux"
  ],
  "librarySettings": {
    "whitelistedNonPeerDependencies": [
      "clipboard-polyfill"
    ]
  },
  "appSettings": {
    "myLibrary": {
      "name": "Skyux Lib Clipboard"
    }
  },
  "app": {
    "title": "Sky Clipboard",
    "styles": []
  },
  "testSettings": {
    "unit": {
      "browserSet": "paranoid"
    }
  },
  "host": {
    "frameOptions": {
      "none": true
    }
  }
}
